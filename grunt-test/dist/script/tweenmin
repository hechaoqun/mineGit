function getCss(a,b){return"scrollTop"==b?a.scrollTop:window.getComputedStyle?parseFloat(getComputedStyle(a,null)[b]):parseFloat(a.currentStyle[b])}function setCss(a,b,c){"opacity"==b?(a.style.opacity=c,a.style.filter="alpha(opacity="+100*c+")"):"scrollTop"==b?a.scrollTop=c:a.style[b]=c+"px"}function animate(ele,obj,duration,type,fnCallback){function _move(){if((times+=interval)>=duration){for(var a in oChange){var b=obj[a];setCss(ele,a,b)}ele.timer=null,"function"==typeof fnCallback&&fnCallback.call(ele)}else{for(var a in oChange){var c=oChange[a],d=oBegin[a],e=fnEffect(times,d,c,duration);setCss(ele,a,e)}ele.timer=window.setTimeout(_move,interval)}}var zhufengEffect={zfLinear:function(t,b,c,d){return c*t/d+b},Quad:{easeIn:function(t,b,c,d){return c*(t/=d)*t+b},easeOut:function(t,b,c,d){return-c*(t/=d)*(t-2)+b},easeInOut:function(t,b,c,d){return(t/=d/2)<1?c/2*t*t+b:-c/2*(--t*(t-2)-1)+b}},Cubic:{easeIn:function(t,b,c,d){return c*(t/=d)*t*t+b},easeOut:function(t,b,c,d){return c*((t=t/d-1)*t*t+1)+b},easeInOut:function(t,b,c,d){return(t/=d/2)<1?c/2*t*t*t+b:c/2*((t-=2)*t*t+2)+b}},Quart:{easeIn:function(t,b,c,d){return c*(t/=d)*t*t*t+b},easeOut:function(t,b,c,d){return-c*((t=t/d-1)*t*t*t-1)+b},easeInOut:function(t,b,c,d){return(t/=d/2)<1?c/2*t*t*t*t+b:-c/2*((t-=2)*t*t*t-2)+b}},Quint:{easeIn:function(t,b,c,d){return c*(t/=d)*t*t*t*t+b},easeOut:function(t,b,c,d){return c*((t=t/d-1)*t*t*t*t+1)+b},easeInOut:function(t,b,c,d){return(t/=d/2)<1?c/2*t*t*t*t*t+b:c/2*((t-=2)*t*t*t*t+2)+b}},Sine:{easeIn:function(t,b,c,d){return-c*Math.cos(t/d*(Math.PI/2))+c+b},easeOut:function(t,b,c,d){return c*Math.sin(t/d*(Math.PI/2))+b},easeInOut:function(t,b,c,d){return-c/2*(Math.cos(Math.PI*t/d)-1)+b}},Expo:{easeIn:function(t,b,c,d){return 0==t?b:c*Math.pow(2,10*(t/d-1))+b},easeOut:function(t,b,c,d){return t==d?b+c:c*(1-Math.pow(2,-10*t/d))+b},easeInOut:function(t,b,c,d){return 0==t?b:t==d?b+c:(t/=d/2)<1?c/2*Math.pow(2,10*(t-1))+b:c/2*(2-Math.pow(2,-10*--t))+b}},Circ:{easeIn:function(t,b,c,d){return-c*(Math.sqrt(1-(t/=d)*t)-1)+b},easeOut:function(t,b,c,d){return c*Math.sqrt(1-(t=t/d-1)*t)+b},easeInOut:function(t,b,c,d){return(t/=d/2)<1?-c/2*(Math.sqrt(1-t*t)-1)+b:c/2*(Math.sqrt(1-(t-=2)*t)+1)+b}},Elastic:{easeIn:function(t,b,c,d,a,p){if(0==t)return b;if(1==(t/=d))return b+c;if(p||(p=.3*d),!a||a<Math.abs(c)){a=c;s=p/4}else var s=p/(2*Math.PI)*Math.asin(c/a);return-a*Math.pow(2,10*(t-=1))*Math.sin((t*d-s)*(2*Math.PI)/p)+b},easeOut:function(t,b,c,d,a,p){if(0==t)return b;if(1==(t/=d))return b+c;if(p||(p=.3*d),!a||a<Math.abs(c)){a=c;s=p/4}else var s=p/(2*Math.PI)*Math.asin(c/a);return a*Math.pow(2,-10*t)*Math.sin((t*d-s)*(2*Math.PI)/p)+c+b},easeInOut:function(t,b,c,d,a,p){if(0==t)return b;if(2==(t/=d/2))return b+c;if(p||(p=d*(.3*1.5)),!a||a<Math.abs(c)){a=c;s=p/4}else var s=p/(2*Math.PI)*Math.asin(c/a);return t<1?a*Math.pow(2,10*(t-=1))*Math.sin((t*d-s)*(2*Math.PI)/p)*-.5+b:a*Math.pow(2,-10*(t-=1))*Math.sin((t*d-s)*(2*Math.PI)/p)*.5+c+b}},Back:{easeIn:function(t,b,c,d,s){return void 0==s&&(s=1.70158),c*(t/=d)*t*((s+1)*t-s)+b},easeOut:function(t,b,c,d,s){return void 0==s&&(s=1.70158),c*((t=t/d-1)*t*((s+1)*t+s)+1)+b},easeInOut:function(t,b,c,d,s){return void 0==s&&(s=1.70158),(t/=d/2)<1?c/2*(t*t*((1+(s*=1.525))*t-s))+b:c/2*((t-=2)*t*((1+(s*=1.525))*t+s)+2)+b}},zfBounce:{easeIn:function(t,b,c,d){return c-zhufengEffect.zfBounce.easeOut(d-t,0,c,d)+b},easeOut:function(t,b,c,d){return(t/=d)<1/2.75?c*(7.5625*t*t)+b:t<2/2.75?c*(7.5625*(t-=1.5/2.75)*t+.75)+b:t<2.5/2.75?c*(7.5625*(t-=2.25/2.75)*t+.9375)+b:c*(7.5625*(t-=2.625/2.75)*t+.984375)+b},easeInOut:function(t,b,c,d){return t<d/2?.5*zhufengEffect.zfBounce.easeIn(2*t,0,c,d)+b:.5*zhufengEffect.zfBounce.easeOut(2*t-d,0,c,d)+.5*c+b}}},fnEffect=zhufengEffect.Expo.easeOut;if("number"==typeof type)switch(type){case 0:break;case 1:fnEffect=zhufengEffect.zfLinear;break;case 2:fnEffect=zhufengEffect.Back.easeOut;break;case 3:fnEffect=zhufengEffect.Elastic.easeOut;break;case 4:fnEffect=zhufengEffect.zfBounce.easeOut}else"function"==typeof type&&(fnCallback=type);var oBegin={},oChange={},flag=0;for(var attr in obj){var begin=getCss(ele,attr),change=obj[attr]-begin;change&&(oBegin[attr]=begin,oChange[attr]=change,flag++)}if(0!==flag){var times=0;if(document.all)interval=15;else var interval=13;window.clearTimeout(ele.timer),_move()}}var DOM={};DOM.listToArray=function(eles){try{a=[].slice.call(eles,0)}catch(e){for(var a=[],i=0;i<eles.length;i++)a.push(eles[i])}return a},DOM.getIndex=function(ele){for(var index=0,p=ele.previousSibling;p;)1==p.nodeType&&index++,p=p.previousSibling;return index},DOM.getElesByClass=function(strClass,context){if((context=context||document).getElementsByClassName)return context.getElementsByClassName(strClass);for(var aClass=strClass.split(/ +/),eles=context.getElementsByTagName("*"),i=0;i<aClass.length;i++)eles=function(className,eles){for(var reg=new RegExp("(?:^| +)"+className+"(?: +|$)"),a=[],i=0;i<eles.length;i++){var ele=eles[i];reg.test(ele.className)&&a.push(ele)}return a}(aClass[i],eles);return eles},DOM.addClass=function(ele,strClass){new RegExp("(?:^| +)"+strClass+"(?: +|$)").test(ele.className)||(ele.className+=" "+strClass)},DOM.removeClass=function(ele,strClass){var reg=new RegExp("(?:^| +)"+strClass+"(?: +|$)","g");ele.className=ele.className.replace(reg,"")},DOM.children=function(ele,tagName){var childNodes=ele.childNodes,a=[];if("string"==typeof tagName){tagName=tagName.toUpperCase();for(var i=0;i<childNodes.length;i++){var child=childNodes[i];1==child.nodeType&&child.nodeName==tagName&&a.push(child)}}return a};